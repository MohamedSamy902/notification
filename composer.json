{
    "name": "mohamedsamy/fcm-notifications",
    "description": "نظام إشعارات Firebase Cloud Messaging متكامل لـ Laravel - Complete FCM Notifications System for Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "firebase",
        "fcm",
        "notifications",
        "push-notifications",
        "cloud-messaging",
        "sweet-alert",
        "web-push"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Samy",
            "email": "mohamedsamy9029@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/MohamedSamy902/notification",
    "support": {
        "issues": "https://github.com/MohamedSamy902/notification/issues",
        "source": "https://github.com/MohamedSamy902/notification"
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MohamedSamy\\FcmNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MohamedSamy\\FcmNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohamedSamy\\FcmNotifications\\FcmNotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
